<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoFrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:orientation="vertical" android:background="@color/color_cc000000" android:layout_width="match_parent" android:layout_height="match_parent">
    <com.zhy.autolayout.AutoFrameLayout android:layout_width="match_parent" android:layout_height="wrap_content">
        <com.zhy.autolayout.AutoLinearLayout android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="100px">
            <com.zhy.autolayout.AutoFrameLayout android:id="@+id/flFeedback" android:paddingLeft="60px" android:paddingRight="60px" android:layout_width="wrap_content" android:layout_height="match_parent" android:layout_marginTop="10px">
                <View android:layout_gravity="bottom|center_vertical|center_horizontal|center" android:background="@drawable/selector_transparent_green" android:layout_width="100px" android:layout_height="8px"/>
                <TextView android:textSize="28px" android:textColor="@color/selector_color_white_white" android:gravity="center" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_marginLeft="21px" android:text="@string/feedback"/>
            </com.zhy.autolayout.AutoFrameLayout>
            <com.zhy.autolayout.AutoFrameLayout android:id="@+id/flContent" android:paddingLeft="60px" android:paddingRight="60px" android:layout_width="wrap_content" android:layout_height="match_parent">
                <View android:layout_gravity="bottom|center_vertical|center_horizontal|center" android:background="@drawable/selector_transparent_green" android:layout_width="100px" android:layout_height="8px"/>
                <TextView android:textSize="28px" android:textColor="@color/selector_color_white_white" android:gravity="center" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_marginLeft="21px" android:text="@string/content_request_land"/>
            </com.zhy.autolayout.AutoFrameLayout>
            <com.zhy.autolayout.AutoFrameLayout android:id="@+id/flService" android:paddingLeft="60px" android:paddingRight="60px" android:layout_width="wrap_content" android:layout_height="match_parent">
                <View android:layout_gravity="bottom|center_vertical|center_horizontal|center" android:background="@drawable/selector_transparent_green" android:layout_width="100px" android:layout_height="8px"/>
                <TextView android:textSize="28px" android:textColor="@color/selector_color_white_white" android:gravity="center" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_marginLeft="21px" android:text="@string/customer_service_land"/>
            </com.zhy.autolayout.AutoFrameLayout>
        </com.zhy.autolayout.AutoLinearLayout>
        <View android:layout_gravity="bottom" android:background="@color/color_666666" android:layout_width="match_parent" android:layout_height="1px"/>
    </com.zhy.autolayout.AutoFrameLayout>
    <FrameLayout android:id="@+id/loading" android:visibility="gone" android:layout_width="match_parent" android:layout_height="match_parent">
        <ProgressBar android:layout_gravity="center" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
    </FrameLayout>
    <include layout="@layout/item_feedback_dialog_feedback"/>
    <include layout="@layout/item_feedback_dialog_content"/>
    <include layout="@layout/item_feedback_dialog_service"/>
    <com.zhy.autolayout.AutoLinearLayout android:layout_gravity="bottom|right|center_vertical|center_horizontal|center" android:orientation="horizontal" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="40px" android:layout_marginBottom="40px">
        <TextView android:textColor="@color/color_ffffff" android:gravity="center" android:id="@+id/tvClose" android:background="@drawable/shape_round_grey" android:layout_width="180px" android:layout_height="80px" android:text="@string/vod_close"/>
        <TextView android:textColor="@color/selector_color_feedback_text" android:gravity="center" android:id="@+id/tvSubmit" android:background="@drawable/selector_btn_feedback_bg" android:layout_width="180px" android:layout_height="80px" android:layout_marginLeft="40px" android:text="@string/submit"/>
    </com.zhy.autolayout.AutoLinearLayout>
</com.zhy.autolayout.AutoFrameLayout>
